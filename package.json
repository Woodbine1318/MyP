{
  "name": "my-paediatrician",
  "private": true,
  "description": "Are you a paediatrician looking for another more effective way of working from home while helping hundreds of parents?",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ignore-path .gitignore",
    "prettier": "prettier . --ignore-path .gitignore",
    "check-format": "yarn prettier --list-different",
    "format": "yarn prettier --write",
    "validate": "yarn check-format && yarn lint && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/pt-sans": "^4.3.0",
    "autoprefixer": "^10.2.5",
    "gatsby": "^3.4.1",
    "gatsby-plugin-gatsby-cloud": "^2.4.1",
    "gatsby-plugin-image": "^1.4.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-offline": "^4.4.0",
    "gatsby-plugin-postcss": "^4.5.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "^3.4.1",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "postcss": "^8.2.15",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "lint-staged": {
    "*.+(js|json|jsx)": [
      "prettier --write",
      "prettier --list-different"
    ],
    "*.(js|jsx)": [
      "eslint"
    ]
  }
}
